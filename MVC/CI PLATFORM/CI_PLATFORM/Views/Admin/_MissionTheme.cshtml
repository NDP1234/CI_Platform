@using CI_Platform.Entities.Models.VM
@model AdminViewModel;
 @foreach (var item in Model.MissionThemes.Where(m=>m.DeletedAt==null))
{
    <tr class="border">
        <td>@item.Title</td>
        @if (@item.Status == 1)
        {
            <td class="greentext">Active</td>
        }
        else
        {
            <td class="redtext">Inactive</td>
        }

        <td class="text-center">
            <button class="border-0 bg-white editMissionThemeModal" type="button"  data-missionTheme-id="@item.MissionThemeId" onclick="OpenEditThemeModal('@item.Title','@item.Status','@item.MissionThemeId')"><i class="bi bi-pencil-square "></i></button>
            <button class="border-0 bg-white deleteMissionTheme" data-missionTheme-id="@item.MissionThemeId"><i class="bi bi-trash  "></i></button>
        </td>

</tr>

}